{
  "name": "svgeditor",
  "version": "1.0.0",
  "description": "svg editor",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'npm run build' 'npm run lite' ",
    "lite": "lite-server -c configs/bs-config.js",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webpack": "webpack --config configs/webpack.config.js --watch",
    "build": "npm run webpack",
    "buildserver": "tsc; babel src -d lib",
    "server": "node lib/server/server.js",
    "dev": "webpack-dev-server --config configs/webpack.dev.js -w",
    "unit": "karma start configs/karma.conf.js --browsers PhantomJS --log-level debug",
    "lint": "tslint ./src/**/*.ts"
  },
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "@angular/upgrade": "^2.4.1",
    "angular-in-memory-web-api": "^0.2.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.2",
    "express": "^4.14.0",
    "mongoose": "^4.7.5",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.41",
    "tslint": "^4.2.0",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel-cli": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "concurrently": "^3.1.0",
    "html-webpack-plugin": "^2.24.1",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "lite-server": "^2.2.2",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.14",
    "raw-loader": "^0.5.1",
    "ts-loader": "^1.3.3",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "typings": "^2.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "keywords": [
    "svg",
    "javascript"
  ],
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "author": "wayming",
  "license": "ISC"
}
